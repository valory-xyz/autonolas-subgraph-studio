{
  "name": "autonolas-tokenomics-subgraph",
  "license": "UNLICENSED",
  "scripts": {
    "build-l1": "yarn codegen-l1 && yarn copy-generated-l1 && yarn build-ethereum",
    "build-l2": "yarn codegen-l2 && yarn copy-generated-l2 && yarn build-arbitrum && yarn build-base && yarn build-celo && yarn build-gnosis && yarn build-mode && yarn build-optimism && yarn build-polygon",
    "codegen-l1": "graph codegen subgraphs/tokenomics/tokenomics-eth/subgraph.yaml",
    "copy-generated-l1": "cp -r generated subgraphs/tokenomics/tokenomics-eth/",
    "build-ethereum": "graph build subgraphs/tokenomics/tokenomics-eth/subgraph.yaml",
    "codegen-l2": "graph codegen subgraphs/tokenomics/tokenomics-base/subgraph.base.yaml --output-dir generated-l2",
    "copy-generated-l2": "rsync -a generated-l2/ shared/tokenomics/generated/ && rm -rf generated-l2",
    "build-arbitrum": "graph build subgraphs/tokenomics/tokenomics-arbitrum/subgraph.arbitrum.yaml",
    "build-base": "graph build subgraphs/tokenomics/tokenomics-base/subgraph.base.yaml",
    "build-celo": "graph build subgraphs/tokenomics/tokenomics-celo/subgraph.celo.yaml",
    "build-gnosis": "graph build subgraphs/tokenomics/tokenomics-gnosis/subgraph.gnosis.yaml",
    "build-mode": "graph build subgraphs/tokenomics/tokenomics-mode/subgraph.mode.yaml",
    "build-optimism": "graph build subgraphs/tokenomics/tokenomics-optimism/subgraph.optimism.yaml",
    "build-polygon": "graph build subgraphs/tokenomics/tokenomics-polygon/subgraph.polygon.yaml",
    "codegen-service-registry": "graph codegen subgraphs/service-registry/service-registry-eth/subgraph.yaml -o shared/service-registry/generated",
    "build-service-registry-l1": "yarn codegen-service-registry && yarn build-service-registry-ethereum",
    "build-service-registry-l2": "yarn codegen-service-registry && yarn build-service-registry-optimism && yarn build-service-registry-base && yarn build-service-registry-gnosis",
    "build-service-registry-ethereum": "graph build subgraphs/service-registry/service-registry-eth/subgraph.yaml",
    "build-service-registry-optimism": "graph build subgraphs/service-registry/service-registry-optimism/subgraph.optimism.yaml",
    "build-service-registry-base": "graph build subgraphs/service-registry/service-registry-base/subgraph.base.yaml",
    "build-service-registry-gnosis": "graph build subgraphs/service-registry/service-registry-gnosis/subgraph.gnosis.yaml",
    "build-service-registry-arbitrum": "graph build subgraphs/service-registry/service-registry-arbitrum/subgraph.arbitrum.yaml",
    "build-service-registry-celo": "graph build subgraphs/service-registry/service-registry-celo/subgraph.celo.yaml",
    "build-service-registry-mode": "graph build subgraphs/service-registry/service-registry-mode/subgraph.mode.yaml",
    "build-service-registry-polygon": "graph build subgraphs/service-registry/service-registry-polygon/subgraph.polygon.yaml",
    "deploy-studio": "node scripts/deploy-studio.js",
    "test": "graph test"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "^0.97.0",
    "@graphprotocol/graph-ts": "^0.38.0"
  },
  "devDependencies": {
    "matchstick-as": "0.5.0"
  }
}