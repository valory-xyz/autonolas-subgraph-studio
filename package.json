{
  "name": "autonolas-tokenomics-subgraph",
  "scripts": {
    "codegen-tokenomics-l1": "graph codegen subgraphs/tokenomics/tokenomics-eth/subgraph.yaml",
    "codegen-tokenomics-l2": "graph codegen subgraphs/tokenomics/tokenomics-base/subgraph.base.yaml -o subgraphs/tokenomics/common/generated",
    "codegen-new-mech-fees-gnosis": "graph codegen subgraphs/new-mech-fees/mech-fees-gnosis/subgraph.yaml -o subgraphs/new-mech-fees/common/generated",
    "codegen-new-mech-fees-base": "graph codegen subgraphs/new-mech-fees/mech-fees-base/subgraph.yaml -o subgraphs/new-mech-fees/common/generated",
    "codegen-predict": "graph codegen subgraphs/predict/subgraph.yaml",
    "codegen-governance": "graph codegen subgraphs/governance/subgraph.yaml",
    "codegen-staking": "graph codegen subgraphs/staking/subgraph.gnosis.yaml",
    "codegen-babydegen-optimism": "graph codegen subgraphs/babydegen/babydegen-optimism/subgraph.yaml",
    "build-tokenomics:ethereum": "yarn codegen-tokenomics-l2 && yarn codegen-tokenomics-l1 && cp -r generated subgraphs/tokenomics/tokenomics-eth/ && graph build subgraphs/tokenomics/tokenomics-eth/subgraph.yaml",
    "build-tokenomics:arbitrum": "yarn codegen-tokenomics-l2 && graph build subgraphs/tokenomics/tokenomics-arbitrum/subgraph.arbitrum.yaml",
    "build-tokenomics:base": "yarn codegen-tokenomics-l2 && graph build subgraphs/tokenomics/tokenomics-base/subgraph.base.yaml",
    "build-tokenomics:celo": "yarn codegen-tokenomics-l2 && graph build subgraphs/tokenomics/tokenomics-celo/subgraph.celo.yaml",
    "build-tokenomics:gnosis": "yarn codegen-tokenomics-l2 && graph build subgraphs/tokenomics/tokenomics-gnosis/subgraph.gnosis.yaml",
    "build-tokenomics:optimism": "yarn codegen-tokenomics-l2 && graph build subgraphs/tokenomics/tokenomics-optimism/subgraph.optimism.yaml",
    "build-tokenomics:polygon": "yarn codegen-tokenomics-l2 && graph build subgraphs/tokenomics/tokenomics-polygon/subgraph.polygon.yaml",
    "build-new-mech-fees:base": "yarn codegen-new-mech-fees-base && graph build subgraphs/new-mech-fees/mech-fees-base/subgraph.yaml",
    "build-new-mech-fees:gnosis": "yarn codegen-new-mech-fees-gnosis && graph build subgraphs/new-mech-fees/mech-fees-gnosis/subgraph.yaml",
    "build-babydegen-optimism": "yarn codegen-babydegen-optimism && graph build subgraphs/babydegen/babydegen-optimism/subgraph.yaml",
    "deploy-studio": "node scripts/deploy-studio.js",
    "test": "graph test"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "^0.97.0",
    "@graphprotocol/graph-ts": "^0.38.0"
  },
  "devDependencies": {
    "matchstick-as": "0.5.0"
  }
}
