{
  "name": "autonolas-tokenomics-subgraph",
  "license": "UNLICENSED",
  "scripts": {
    "build-l1": "yarn codegen-ethereum && yarn copy-generated-l1 && yarn build-ethereum",
    "build-l2": "yarn codegen-base && yarn build-base && yarn codegen-gnosis && yarn build-gnosis",
    "codegen-ethereum": "graph codegen subgraphs/tokenomics/subgraph.yaml",
    "copy-generated-l1": "cp -r generated subgraphs/tokenomics/",
    "build-ethereum": "graph build subgraphs/tokenomics/subgraph.yaml",
    "codegen-base": "graph codegen subgraphs/tokenomics-base/subgraph.base.yaml",
    "build-base": "graph build subgraphs/tokenomics-base/subgraph.base.yaml",
    "codegen-gnosis": "graph codegen subgraphs/tokenomics-gnosis/subgraph.gnosis.yaml",
    "build-gnosis": "graph build subgraphs/tokenomics-gnosis/subgraph.gnosis.yaml",
    "deploy-studio": "node scripts/deploy-studio.js",
    "test": "graph test"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "^0.97.0",
    "@graphprotocol/graph-ts": "^0.38.0"
  },
  "devDependencies": {
    "matchstick-as": "0.5.0"
  }
}