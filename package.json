{
  "name": "autonolas-tokenomics-subgraph",
  "license": "UNLICENSED",
  "scripts": {
    "build-l1": "yarn codegen-l1 && yarn copy-generated-l1 && yarn build-ethereum",
    "build-l2": "yarn codegen-l2 && yarn copy-generated-l2 && yarn build-base && yarn build-gnosis",
    "build-service-registry": "yarn codegen-service-registry && yarn copy-generated-service-registry && yarn build-service-registry-subgraph",
    "codegen-l1": "graph codegen subgraphs/tokenomics/tokenomics-eth/subgraph.yaml",
    "copy-generated-l1": "cp -r generated subgraphs/tokenomics/tokenomics-eth/",
    "build-ethereum": "graph build subgraphs/tokenomics/tokenomics-eth/subgraph.yaml",
    "codegen-l2": "graph codegen subgraphs/tokenomics/tokenomics-base/subgraph.base.yaml --output-dir generated-l2",
    "copy-generated-l2": "cp -r generated-l2/* shared/tokenomics/generated/",
    "build-base": "graph build subgraphs/tokenomics/tokenomics-base/subgraph.base.yaml",
    "build-gnosis": "graph build subgraphs/tokenomics/tokenomics-gnosis/subgraph.gnosis.yaml",
    "codegen-service-registry": "graph codegen subgraphs/service-registry/service-registry-eth/subgraph.yaml",
    "copy-generated-service-registry": "cp -r generated subgraphs/service-registry/service-registry-eth/",
    "build-service-registry-subgraph": "graph build subgraphs/service-registry/service-registry-eth/subgraph.yaml",
    "deploy-studio": "node scripts/deploy-studio.js",
    "test": "graph test"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "^0.97.0",
    "@graphprotocol/graph-ts": "^0.38.0",
    "ethers": "^6.15.0"
  },
  "devDependencies": {
    "matchstick-as": "0.5.0"
  }
}