{
  "name": "autonolas-tokenomics-subgraph",
  "license": "UNLICENSED",
  "scripts": {
    "build-l1": "yarn codegen-l1 && yarn copy-generated-l1 && yarn build-ethereum",
    "build-l2": "yarn codegen-l2 && yarn copy-generated-l2 && yarn build-arbitrum && yarn build-base && yarn build-celo && yarn build-gnosis && yarn build-mode && yarn build-optimism && yarn build-polygon",
    "codegen-l1": "graph codegen subgraphs/tokenomics/subgraph.yaml",
    "copy-generated-l1": "cp -r generated subgraphs/tokenomics/",
    "build-ethereum": "graph build subgraphs/tokenomics/subgraph.yaml",
    "codegen-l2": "graph codegen subgraphs/tokenomics-base/subgraph.base.yaml --output-dir generated-l2",
    "copy-generated-l2": "rsync -a generated-l2/ shared/tokenomics/generated/ && rm -rf generated-l2",
    "build-arbitrum": "graph build subgraphs/tokenomics-arbitrum/subgraph.arbitrum.yaml",
    "build-base": "graph build subgraphs/tokenomics-base/subgraph.base.yaml",
    "build-celo": "graph build subgraphs/tokenomics-celo/subgraph.celo.yaml",
    "build-gnosis": "graph build subgraphs/tokenomics-gnosis/subgraph.gnosis.yaml",
    "build-mode": "graph build subgraphs/tokenomics-mode/subgraph.mode.yaml",
    "build-optimism": "graph build subgraphs/tokenomics-optimism/subgraph.optimism.yaml",
    "build-polygon": "graph build subgraphs/tokenomics-polygon/subgraph.polygon.yaml",
    "deploy-studio": "node scripts/deploy-studio.js",
    "test": "graph test"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "^0.97.0",
    "@graphprotocol/graph-ts": "^0.38.0"
  },
  "devDependencies": {
    "matchstick-as": "0.5.0"
  }
}